<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-razoyo-carprofile" ifconfig="razoyo_carprofile/carprofile_config/enabled">
                <arguments>
                    <argument name="path" xsi:type="string">carprofile/profile/index</argument>
                    <argument name="label" xsi:type="string">My Car Profile</argument>
                    <argument name="sortOrder" xsi:type="number">210</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
